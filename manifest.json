{
    "manifest_version": 3,
    "name": "image text ai",
    "version": "1.0",
    "description": "Dynamically adds 'alt' text to images missing alt attributes.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }